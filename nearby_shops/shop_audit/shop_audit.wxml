<wxs module="util" src="../../utils/util.wxs" />
<view class='warp'>
  <view class='warpbg' style='background: linear-gradient(to right, {{diyColor.zColor}}, {{diyColor.fColor6}});'>
    <image class='bg-2' src='{{util.HTTP}}mobile/small/image/bank/icon_100.png'></image>
    <!-- <image class='bg-2' src='{{util.HTTP}}mobile/small/image/fx/dysh-q.png'></image> -->
    <image class='bg-3' src='{{util.HTTP}}mobile/small/image/fx/dysh-bj.png'></image>
  </view>
  <view class='warp_con'>
    <view class='title' style='{{util.diyColor(diyColor.zColor,"","")}}'>{{data.arr[0]}}</view>
    <view class='tip'>{{data.arr[1]}}</view>
    <view class='tip'>*注登录账号为您绑定会员手机号</view>
  </view>
  <text class='goHome' hover-class='hover-class' hover-stay-time='400' bindtap='copy' style='{{util.diyColor("",diyColor.zColor,"")}}'>复制网址</text>
</view>
