page {
  background-color: #F5F5F5;
}

.wrap {
  padding: 70px 38rpx 0;
  text-align: center;
}

.big-title {
  font-size: 48rpx;
  font-weight: bold;
  line-height: 64rpx;
  color: #000;
}

.password-input {
  display: block;
  width: 100%;
  margin: 118rpx 0 56rpx;
  border: 1rpx solid #999999;
  height: 96rpx;
  background-color: #fff;
  text-align: left;
}

.exchange-btn {
  line-height: 96rpx;
  font-size: 32rpx;
  margin-bottom: 174rpx;
  background-color: #981D0E;
  color: #fff;
}

.desc-icon {
  margin: auto;
  width: 54rpx;
  height: 52rpx;
  background: url(data:image/png;base64,) center / 100% 100%;
}

.desc-title {
  margin: 8rpx 0 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  line-height: 42rpx;
  color: #666;
}

.desc-title .text {
  margin: 0 20rpx;
}

.desc-title .line {
  width: 10rpx;
  height: 1px;
  background-color: #666;
}

.desc-wrap {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #999;
  text-align: left;
}

.success-modal {
  position: fixed;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.56);
}

.success-modal .modal-wrap {
  width: 642rpx;
  height: 720rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}

.success-modal .modal-wrap image {
  width: 642rpx;
  height: 720rpx;
}

.success-modal .modal-wrap .amout-show {
  position: absolute;
  top: 196rpx;
  left: 50%;
  transform: translate(-50%, 0);
  color: #FFE135;
  font-size: 60rpx;
  line-height: 88rpx;
}

.success-modal .modal-wrap navigator {
  position: absolute;
  top: 280rpx;
  left: 50%;
  transform: translate(-50%, 0);
  color: #FFCF97;
  font-size: 24rpx;
  line-height: 34rpx;
}